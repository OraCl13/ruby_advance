$('.questions').html('<%= j render 'questions/questions', questions: Question.all %>');
$('.edit-question-link').click(function(e) {
    e.preventDefault();
    $(this).hide();
    question_id = $(this).data('questionId');
    console.log(question_id)
    $('form#edit-question-' + question_id).show();
    $('form#edit-question-' + question_id).submit(function(){
        $(this).hide();
    })
});